<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5level %logger{36}: %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="webnf.util-test" level="DEBUG" />

  <root level="TRACE">
    <appender-ref ref="STDOUT" />
  </root>

</configuration>
